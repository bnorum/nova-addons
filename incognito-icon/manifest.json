{
    "manifest_version": 2,
    "name": "Incognito Window Opener",
    "version": "1.0",
    "description": "Opens a new private browsing window when clicked.",
    "icons": {
      "48": "icon.png"
    },
    "browser_action": {
      "default_icon": "icon.png",
      "default_title": "Open Incognito Window"
    },
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "permissions": ["cookies"]
  }
  